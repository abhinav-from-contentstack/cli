{
  "name": "@contentstack/cli-2",
  "description": "Command-line tool (CLI) to interact with Contentstack",
  "version": "0.1.1-beta.8",
  "author": "Contentstack",
  "bin": {
    "csdx2": "./bin/run"
  },
  "main": "src/index.js",
  "scripts": {
    "build": "npm run clean && npm run compile",
    "clean": "rm -rf ./lib && rm -rf tsconfig.build.tsbuildinfo",
    "compile": "tsc -b tsconfig.json",
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "npm run build && oclif-dev manifest && oclif-dev readme",
    "version": "oclif-dev readme && git add README.md",
    "test:report": "tsc -p test && nyc --reporter=lcov --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "pretest": "tsc -p test",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "posttest": "npm run lint",
    "lint": "eslint src/**/*.ts"
  },
  "dependencies": {
    "@contentstack/cli-command": "0.1.1-beta.1",
    "@contentstack/cli-config": "0.1.1-beta.1",
    "@contentstack/management": "1.2.1",
    "@contentstack/utilities": "0.1.1-beta.1",
    "@oclif/command": "^1.6.1",
    "@oclif/config": "^1.15.1",
    "@oclif/errors": "^1.3.3",
    "@oclif/plugin-help": "^2.2.3",
    "@oclif/plugin-not-found": "^1.2.3",
    "@oclif/plugin-plugins": "^1.8.1",
    "axios": "^0.21.1",
    "chalk": "^4.0.0",
    "cli-ux": "^5.4.6",
    "configstore": "^5.0.1",
    "debug": "^4.1.1",
    "figlet": "^1.5.0",
    "inquirer": "^7.1.0",
    "querystring": "^0.2.1",
    "uuid": "^8.3.2",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2",
    "@oclif/plugin-help": "^3.2.0",
    "@oclif/test": "^1.2.8",
    "@types/chai": "^4.2.18",
    "@types/inquirer": "^7.3.1",
    "@types/mkdirp": "^1.0.1",
    "@types/mocha": "^8.2.2",
    "@types/nock": "^11.1.0",
    "@types/node": "^14.14.32",
    "@types/sinon": "^10.0.2",
    "@types/tar": "^4.0.3",
    "@types/winston": "^2.4.4",
    "chai": "^4.3.4",
    "eslint": "^5.16.0",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-oclif-typescript": "^0.1.0",
    "globby": "^10.0.2",
    "mocha": "^8.3.2",
    "nock": "^13.1.0",
    "nyc": "^15.1.0",
    "rimraf": "^2.7.1",
    "sinon": "^11.1.1",
    "tmp": "^0.2.1",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/contentstack/cli",
  "keywords": [
    "contentstack",
    "cli",
    "plugin"
  ],
  "license": "MIT",
  "oclif": {
    "topics": {
      "auth": {
        "description": "perform authentication-related activities"
      },
      "cm": {
        "description": "perform content management activities"
      },
      "config": {
        "description": "perform configuration related activities"
      },
      "plugins": {
        "description": "list installed plugins"
      }
    },
    "helpClass": "./lib/help.js",
    "commands": "./lib/commands",
    "bin": "csdx",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-not-found",
      "@oclif/plugin-plugins",
      "@contentstack/cli-config"
    ],
    "hooks": {
      "prerun": [
        "./lib/hooks/prerun/print-region",
        "./lib/hooks/prerun/validate-auth-token"
      ],
      "init": [
        "./lib/hooks/init/context-init",
        "./lib/hooks/init/utils-init"
      ],
      "postrun": [
        "./lib/hooks/postrun/analytics"
      ]
    }
  },
  "repository": "https://github.com/contentstack/cli",
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  }
}